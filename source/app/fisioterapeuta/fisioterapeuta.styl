        url('../../fonts/fonts/icomoon.ttf?djvnsd') format('truetype'),
        url('../../fonts/fonts/icomoon.woff?djvnsd') format('woff'),
        url('../../fonts/fonts/icomoon.svg?djvnsd#icomoon') format('svg')
    font-weight normal
    font-style normal


[class^="icon-"], [class*=" icon-"]
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family 'icomoon' !important
    speak none
    font-style normal
